{
    "manifest_version": 2,
    "name": "videolator",
    "version": "2.5",
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/watch?v=*"],
        "js": ["jquery.min.js", "inject.js"]
    }
    ],
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_icon" : "img/icon.png"
    },
    "permissions": [ "tabs" ]
  }